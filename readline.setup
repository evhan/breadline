(compile -s -O3 -d1 "readline.scm" -J -lreadline)
(compile -s "readline.import.scm")

(compile -s -O3 -d1 "readline-scheme-completion.scm" -J)
(compile -s "readline-scheme-completion.import.scm")

(install-extension
 'readline
 '("readline.so" "readline.import.so"
   "readline-scheme-completion.so" "readline-scheme-completion.import.so")
 '((version "0.0.1")))
