(compile -s -O3 -d1 "breadline.scm" -J -lreadline)
(compile -s "breadline.import.scm")

(compile -s -O3 -d1 "breadline-scheme-completion.scm" -J)
(compile -s "breadline-scheme-completion.import.scm")

(install-extension
 'readline
 '("breadline.so" "breadline.import.so"
   "breadline-scheme-completion.so" "breadline-scheme-completion.import.so")
 '((version "0.0.1")))
